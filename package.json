{
  "name": "Grivaty_wallet",
  "version": "1.0.0",
  "description": "Wallet using electron",
  "main": "main.js",
  "build": {
    "appId": "com.dzmy.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "start": "electron .",
