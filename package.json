  },
  "dependencies": {
    "ejs": "^2.6.1",
    "electron-json-config": "^1.5.3",
    "electron-settings": "^3.2.0",
    "qrcode": "^1.2.2",
    "sqlite3": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git@10.20.1.1:SHDMT/gwallet-gui.git"
